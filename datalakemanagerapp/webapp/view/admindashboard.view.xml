<mvc:View controllerName="co.haina.datalakemanagerapp.controller.admindashboard"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt" height="100%" class="side-navigation-demo">
    <Button id="_IDGenButton1" text="Toggle Collapse/Expand" icon="sap-icon://menu2" press=".onCollapseExpandPress" />
    <tnt:ToolPage id="toolPage">
        <tnt:sideContent>                        
            <tnt:SideNavigation id="sideNavigation" itemSelect="onItemSelect">
                <tnt:NavigationList id="_IDGenNavigationList1">
                    <tnt:NavigationListItem id="_IDGenNavigationListItem11"  text="{i18n>Monitor}" icon="sap-icon://sys-monitor">
                        <tnt:NavigationListItem id="_IDGenNavigationListItem12"  text="{i18n>MonitorReading}" key="rtMonitor" />                        
                    </tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem1" text="{i18n>upload}" icon="sap-icon://add-document">
                        <tnt:NavigationListItem id="_IDGenNavigationListItem2" text="{i18n>uploadsource}" key="rtUploadsource" />
                        <tnt:NavigationListItem id="_IDGenNavigationListItem3" text="{i18n>uploadreadings}" key="rtUploadreadings" />
                    </tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem4" text="{i18n>log}" icon="sap-icon://it-host">
                        <tnt:NavigationListItem text="{i18n>logapp}" id="logapp" key="rtLogapp" />
                        <tnt:NavigationListItem text="{i18n>logupload}" id="logupload" key="rtLogupload" />
                    </tnt:NavigationListItem>
                    <tnt:NavigationListItem id="_IDGenNavigationListItem5" text="{i18n>setting}" icon="sap-icon://user-settings">
                        <tnt:NavigationListItem id="_IDGenNavigationListItem6" text="{i18n>meter}" />
                        <tnt:NavigationListItem id="_IDGenNavigationListItem7" text="{i18n>parameters}" />
                        <tnt:NavigationListItem id="_IDGenNavigationListItem8" text="{i18n>uploadConf}" />
                    </tnt:NavigationListItem>
                </tnt:NavigationList>
                <tnt:fixedItem>
                    <tnt:NavigationList id="_IDGenNavigationList2">
                        <tnt:NavigationListItem id="_navItLogin" text="{i18n>logging}" key="rtlogging" icon="sap-icon://role" />
                        <tnt:NavigationListItem id="_IDGenNavigationListItem9" text="{i18n>logapp}" key="rtLogapp" />
                        <tnt:NavigationListItem id="_IDGenNavigationListItem10" text="{i18n>parameters}" icon="sap-icon://settings" />
                    </tnt:NavigationList>
                </tnt:fixedItem>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <App id="parentNavContainer"/>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
