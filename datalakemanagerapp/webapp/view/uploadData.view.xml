<mvc:View xmlns:vbm="sap.ui.vbm" controllerName="co.haina.datalakemanagerapp.controller.uploadData"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns:m="sap.m"
	xmlns:u="sap.ui.unified">
	<m:Page id="page" title="{viewModel>/title}" busy="{viewModel>/busy}" busyIndicatorDelay="0" >
		<m:content>
			<m:VBox id="_IDGenVBox1">
				<m:Panel id="_IDGenPanel1" headerText="OC">
					<u:FileUploader id="Factornodocapacidad" name="Factornodocapacidad" buttonText="{i18n>Factornodocapacidad}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="InyeccionesFisicasDeEnergiaPorPuntoDeSuministro" name="InyeccionesFisicasDeEnergiaPorPuntoDeSuministro" buttonText="{i18n>InyeccionesFisicasDeEnergiaPorPuntoDeSuministro}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="PotenciaFirme" name="PotenciaFirme" buttonText="{i18n>PotenciaFirme}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="CargoTotalPorPlanta" name="CargoTotalPorPlanta" buttonText="{i18n>CargoTotalPorPlanta}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="CompensacionTotalPorPlanta" name="CompensacionTotalPorPlanta" buttonText="{i18n>CompensacionTotalPorPlanta}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="CargoPorDesviosDePotenciaHoraria" name="CargoPorDesviosDePotenciaHoraria" buttonText="{i18n>CargoPorDesviosDePotenciaHoraria}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="RetiroDeEnergiaPorContratos" name="RetiroDeEnergiaPorContratos" buttonText="{i18n>RetiroDeEnergiaPorContratos}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="RetirosFisicosDeEnergiaPorPuntoDeSuministro" name="RetirosFisicosDeEnergiaPorPuntoDeSuministro" buttonText="{i18n>RetirosFisicosDeEnergiaPorPuntoDeSuministro}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="RetiroPotenciaContratada" name="RetiroPotenciaContratada" buttonText="{i18n>RetiroPotenciaContratada}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="InyeccionesPorContratosDePotenciaPreliminar" name="InyeccionesPorContratosDePotenciaPreliminar" buttonText="{i18n>InyeccionesPorContratosDePotenciaPreliminar}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="ReporteCMGporBarraSMC" name="ReporteCMGporBarraSMC" buttonText="{i18n>ReporteCMGporBarraSMC}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="CostoMarginalDePotenciaDePunta" name="CostoMarginalDePotenciaDePunta" buttonText="{i18n>CostoMarginalDePotenciaDePunta}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
				</m:Panel>

				<m:Panel id="_IDGenPanel2" headerText="SAGC (Suite de Aplicaciones de la Gerencia Comercial)">
					<m:Label text="{i18n>period}: " labelFor="FilePeriod" id="lblPeriod" />
					<m:DatePicker valueState="None"
					id="FilePeriod"
					width="15%"
					value="{
						'path': 'viewModel>/valueFilePeriod',
						'type': 'sap.ui.model.type.Date',
						'formatOptions': {
							'pattern': 'M/y'
						}
					}"
					class="sapUiSmallMarginBottom"/>
					<u:FileUploader id="Derechoconexionunitario" name="Derechoconexionunitario" buttonText="{i18n>Derechoconexionunitario}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="compensacionDesvio" name="compensacionDesvio" buttonText="{i18n>compensacionDesvio}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
					<u:FileUploader id="CargoscompensacionDespachoforzadoSIE" name="CargoscompensacionDespachoforzadoSIE" buttonText="{i18n>CargoscompensacionDespachoforzadoSIE}" icon="sap-icon://sys-prev-page" tooltip="{i18n>Importar}" uploadComplete="onFileUploadComplete" change="onFileUpload" sameFilenameAllowed="true"/>
				</m:Panel>
			</m:VBox>
		</m:content>
	</m:Page>
</mvc:View>