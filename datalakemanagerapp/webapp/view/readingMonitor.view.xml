<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="co.haina.datalakemanagerapp.controller.readingMonitor"
    xmlns="sap.m"
    xmlns:jsed="sap.ui.integration.widgets"
    xmlns:layout="sap.ui.layout"
    xmlns:ui="sap.ui.table">
    <Page showHeader="true" title="{i18n>MonitorReading}" showFooter="true" showNavButton="false" busy="{viewModel>busy}" >
        <content>
            <IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true" selectedKey="Ok">
                <items>
                    <IconTabFilter icon="sap-icon://complete" iconColor="Default" text="Completas" count="{/CompleteProfileValuesCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
                        <content>
                            <ui:Table id="tblComplete" rows="{/CompleteProfileValues}" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true" class="sapUiResponsiveMargin">

                                <ui:extension>
                                    <OverflowToolbar width="100%" height="auto" design="Transparent" visible="true" enabled="true">
                                        <content>
                                            <Label text="{i18n>dateFrom}" labelFor="dtFrom"/>
                                            <DatePicker id="dtFrom" width="22%" value="{
                                                            'path':'viewModel>/valueDateFrom',
                                                            'type':'sap.ui.model.type.Date',
                                                            'formatOptions': {
                                                                'pattern': 'yyyy-MM-dd'
                                                            }
                                                        }" class="sapUiSmallMarginBottom"/>
                                            <Label text="{i18n>dateTo}" labelFor="dtFrom"/>
                                            <DatePicker id="dtTo" width="22%" value="{
                                                            'path':'viewModel>/valueDateTo',
                                                            'type':'sap.ui.model.type.Date',
                                                            'formatOptions': {
                                                                'pattern': 'yyyy-MM-dd'
                                                            }
                                                        }" class="sapUiSmallMarginBottom"/>

                                            <Input id="inpProfile" type="Text" value="000000000000100005" />
                                            <Button icon="sap-icon://search" id="btnSearch" text="{i18n>search}" press="_getCompleteProfileValues" />
                                            <Text text="Items ({/CompleteProfileValuesCount})" width="auto" maxLines="1" wrapping="false" textAlign="End" textDirection="Inherit" visible="true"/>
                                        </content>
                                    </OverflowToolbar>
                                </ui:extension>
                                <ui:columns>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <Label text="{i18n>serialNo1}" />
                                        <ui:template>
                                            <Text text="{Profile}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                    <ui:Column width="170px" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="WithoutHeader" mergeDuplicates="false">
                                        <Label text="{i18n>descSerialNo1}"/>
                                        <ui:template>
                                            <Text text="{ProfileText}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <Label text="{i18n>serialNo2}"/>
                                        <ui:template>
                                            <Text text="{Profile}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">

                                        <Label text="{i18n>descSerialNo1}" />
                                        <ui:template>
                                            <Text text="{ProfileText}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">

                                        <Label text="{i18n>profDate}" />
                                        <ui:template>
                                            <Text text="{ProfDate}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">

                                        <Label text="{i18n>interval}" />
                                        <ui:template>
                                            <Text text="{ProfTime}" wrapping="false"/>
                                        </ui:template>

                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Block" mergeDuplicates="false">

                                        <Label text="{i18n>ProfValue1}" />
                                        <ui:template>
                                            <Text text="{Value}" wrapping="false"/>
                                        </ui:template>

                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="false" popinDisplay="Inline" mergeDuplicates="false">

                                        <Label text="{i18n>ProfValue2}" />
                                        <ui:template>
                                            <Text text="{Value}" wrapping="false"/>
                                        </ui:template>

                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="false" popinDisplay="Inline" mergeDuplicates="false">

                                        <Label text="{i18n>ProfValueContingency}" />
                                        <ui:template>
                                            <Text text="{ContingencyValue}" wrapping="false"/>
                                        </ui:template>

                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <Label text="{i18n>UOM}" />
                                        <ui:template>
                                            <Text text="{UnitOfMeasure}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                    <ui:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <Label text="{i18n>shippingDate}" />
                                        <ui:template>
                                            <Text text="{SAPshippingDate}" wrapping="false"/>
                                        </ui:template>
                                    </ui:Column>
                                </ui:columns>
                            </ui:Table>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter icon="sap-icon://error" iconColor="Default" text="{i18n>MissingProfileValues}" count="{/MissingProfileValuesCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
                        <content>
                            <ui:Table id="tblMissing" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true" class="sapUiResponsiveMargin">
                                <ui:extension>
                                    <OverflowToolbar width="auto" height="auto" design="Transparent" visible="true" enabled="true">
                                        <content>
                                            <Label text="{i18n>dateFrom}" labelFor="dtFromM"/>
                                            <DatePicker id="dtFromM" width="20%" value="{
                                                            'path':'viewModel>/valueDateFrom',
                                                            'type':'sap.ui.model.type.Date',
                                                            'formatOptions': {
                                                                'pattern': 'yyyy-MM-dd'
                                                            }
                                                        }" class="sapUiSmallMarginBottom"/>
                                            <Label text="{i18n>dateTo}" labelFor="dtFromM"/>
                                            <DatePicker id="dtToM" width="20%" value="{
                                                            'path':'viewModel>/valueDateTo',
                                                            'type':'sap.ui.model.type.Date',
                                                            'formatOptions': {
                                                                'pattern': 'yyyy-MM-dd'
                                                            }
                                                        }" class="sapUiSmallMarginBottom"/>
                                            <Label text="{i18n>serialNo}" labelFor="inpSerialNo"/>
                                            <Input id="inpSerialNo" type="Text" width="20%" />                                    
                                            <Button icon="sap-icon://search" text="{i18n>search}" press="_getMissingReading" />
                                        </content>
                                    </OverflowToolbar>
                                </ui:extension>
                            </ui:Table>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter icon="sap-icon://direction-arrows" iconColor="Critical" text="{i18n>InconsistentProfileValues}" count="{/InconsistentProfileValuesCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
                        <content>
                            <ui:Table id="tblInconsistent" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true" class="sapUiResponsiveMargin">
                                <ui:extension>
                                    <OverflowToolbar width="auto" height="auto" design="Transparent" visible="true" enabled="true">
                                        <content>
                                            <Label text="{i18n>dateFrom}" labelFor="dtFromM"/>
                                            <DatePicker id="dtFromI" width="20%" value="{
                                                            'path':'viewModel>/valueDateFromI',
                                                            'type':'sap.ui.model.type.Date',
                                                            'formatOptions': {
                                                                'pattern': 'yyyy-MM-dd'
                                                            }
                                                        }" class="sapUiSmallMarginBottom"/>
                                            <Label text="{i18n>dateTo}" labelFor="dtFromM"/>
                                            <DatePicker id="dtToI" width="20%" value="{
                                                            'path':'viewModel>/valueDateToI',
                                                            'type':'sap.ui.model.type.Date',
                                                            'formatOptions': {
                                                                'pattern': 'yyyy-MM-dd'
                                                            }
                                                        }" class="sapUiSmallMarginBottom"/>
                                            <Button icon="sap-icon://search" text="{i18n>search}" press="_getInconsistentReading" />
                                        </content>
                                    </OverflowToolbar>
                                </ui:extension>
                            </ui:Table>
                        </content>
                    </IconTabFilter>
